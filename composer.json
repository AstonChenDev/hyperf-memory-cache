{
    "name": "aston/memory-cache",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aston\\MemoryCache\\": "src/"
        }
    },
    "description": "基于 Swoole Hyperf 框架，将 SwooleTable 作为 Redis 的上级缓存",
    "authors": [
        {
            "name": "ChenPeng",
            "email": "chenpeng@kiwiing.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "hyperf/process": "^2.2",
        "hyperf/event": "^2.2",
        "psr/container": "^1.0|^2.0",
        "hyperf/utils": "~2.2.0",
        "hyperf/redis": "2.2.*",
        "hyperf/di": "2.2.*",
        "hyperf/config": "2.2.*",
        "ext-redis": "*",
        "psr/log": "*"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "^4.2"
    },
    "extra": {
        "hyperf": {
            "config": "Aston\\MemoryCache\\ConfigProvider"
        }
    }
}
